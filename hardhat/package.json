    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.3",
    "hardhat": "^2.2.0",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.7.5",
    "jest": "^26.6.3",
    "mocha": "^8.3.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.2",
